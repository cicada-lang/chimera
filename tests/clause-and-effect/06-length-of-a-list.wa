import { Zero, Add1, One, Two } from "../../std/datatypes/nat.wa"

Length [[], n]
-------------------- {
  Zero n
}

Length [[head, ...tail], n]
-------------------------------- {
  Length [tail, prev]
  Add1 [prev, n]
}

query n {
  Length [["apple", "pear"], n]
}

query list { Length [list, n] Zero n }
query list { Length [list, n] One n }
query list { Length [list, n] Two n }

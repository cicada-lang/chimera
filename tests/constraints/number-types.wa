assert find _ {
  Number 1
}

assert not find _ {
  Number "A"
}

assert not find x {
  x = "cat"
  Number x
}

assert find x {
  x = 1
  Number x
}

assert not find x {
  Number x
  String x
}

// The inequality is subsumed by type constraint:

find x {
  x != "cat"
  Number x
}

find [x, y] {
  [x, y] != ["cat", 1]
  Number x
}

find x {
  Number x
  x != "cat"
}

find [x, y] {
  Number x
  [x, y] != ["cat", 1]
}

find x {
  Number z
  z = x
  x != "cat"
}

datatype List {
  null
  cons(head, tail) -- { List tail }
}

// assert not find _ {
//   List List::cons(3, "no a List")
// }

find _ {
  List List::null
  List List::cons(1, List::cons(2, List::cons(3, List::null)))
}

find [list, x, y, z] {
  list = List::cons(1, List::cons(2, List::cons(3, List::null)))
  list = List::cons(x, List::cons(y, List::cons(z, List::null)))
}

find [list, x, y, z] {
  list = List::cons(x, List::cons(y, List::cons(z, List::null)))
}

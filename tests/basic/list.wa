find abc {
  abc = ["a", "b", "c"]
}

find [a, b, c] {
  [a, b, c] = ["a", "b", "c"]
}

find empty {
  empty = []
}

find [head, rest] {
  [head, ...rest] = ["a", "b", "c"]
}

find [head, rest] {
  [head, ...rest] = ["a", ..."b"]
}

find [head, rest, result] {
  [head, ...rest] = ["a", ..."b"]
  result = [head, ...rest]
}

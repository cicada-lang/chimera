query abc {
  abc = ["a", "b", "c"]
}

query [a, b, c] {
  [a, b, c] = ["a", "b", "c"]
}

query empty {
  empty = []
}

query [head, rest] {
  [head, ...rest] = ["a", "b", "c"]
}

query [head, rest] {
  [head, ...rest] = ["a", ..."b"]
}

query [head, rest, result] {
  [head, ...rest] = ["a", ..."b"]
  result = [head, ...rest]
}

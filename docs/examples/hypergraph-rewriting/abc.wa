hyperrule abc {
  ["a", "b"] => ["c"]
  ["a", "c"] => ["b"]
  ["b", "c"] => ["a"]
}

abc(quote ["a", "a", "a"])

abc(quote [
  "a", "b", "b", "a",
])

abc(quote [
  "a", "b", "b", "a",
  "a",
])

abc(quote [
  "a", "b", "b", "a",
  "a", "a",
])

abc(quote [
  "a", "b", "b", "a",
  "a", "a", "a",
])

import { Zero, Add1, One, Two } from "../../../std/nat/index.wa"

Length [[], n]
-------------------- {
  Zero n
}

Length [[head, ...tail], n]
-------------------------------- {
  Length [tail, prev]
  Add1 [prev, n]
}

find n {
  Length [["apple", "pear"], n]
}

find list { Length [list, n] Zero n }
find list { Length [list, n] One n }
find list { Length [list, n] Two n }

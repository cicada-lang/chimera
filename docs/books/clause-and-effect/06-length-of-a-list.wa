import { Zero, Add1, One, Two } from "../../std/nat/index.wa"

clause Length([], n)
-------------------- {
  Zero(n)
}

clause Length([_head | tail], n)
-------------------------------- {
  Add1(prev, n)
  Length(tail, prev)
}


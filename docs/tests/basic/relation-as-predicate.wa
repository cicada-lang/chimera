clause Exp(var(name)) -- { String(name) }
clause Exp(fn(name, ret)) -- { String(name) Exp(ret) }
clause Exp(ap(target, arg)) -- { Exp(target) Exp(arg) }

// true

print Exp(quote var("x"))
print Exp(quote fn("x", var("x")))

// false

print Exp(quote "x")
print Exp(quote fn())

// true

print Equal({ x: 1 }, { x: 1 })

// false

print NotEqual({ x: 1 }, { x: 1 })

// true

print Boolean(true)
print Boolean(false)
print Null(null)

// false

print Boolean(1)
print Boolean(1)
print Null(1)

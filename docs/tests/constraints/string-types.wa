find _ {
  String("A")
}

find _ {
  String(1)
}

find x {
  x = "cat"
  String(x)
}

// Reification.

find x {
  String(x)
}

// An unification breaks a type constraint.

find x {
  String(x)
  x = 1
}

// An indirect unification breaks a type constraint.

find x {
  String(x)
  x = z
  z = 1
}

find x {
  x = z
  z = 1
  String(x)
}

find x {
  x = z
  String(x)
  z = 1
}

// An unification subsums a type constraint.

find x {
  String(x)
  x = "A"
}

// An indirect unification subsums a type constraint.

find x {
  String(x)
  x = z
  z = "A"
}

find x {
  x = z
  z = "A"
  String(x)
}

find x {
  x = z
  String(x)
  z = "A"
}

() => {
  return 1
}

(x) => {
  let y = x
  return y
}

// id

let id = (x) => {
  let y = x
  return y
}

id(1)
id(id(1))
id(id(id(1)))

// zero arity

let one = () => {
  return 1
}

one()

// relation in function

let greet = (x) => {
  clause Hi("hi")

  // The expressions in `find` are implicitly `quote`d.

  find q {
    Hi(p)
    q = [p, unquote x]
  }

  return "ok"
}

greet("xieyuheng")

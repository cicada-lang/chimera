eval () => {
  return 1
}

eval (x) => {
  let y = x
  return y
}

// id

let id = (x) => {
  let y = x
  return y
}

eval id(1)
eval id(id(1))
eval id(id(id(1)))

// zero arity

let one = () => {
  return 1
}

eval one()

// relation in function

let greet = (x) => {
  clause Hi("hi")

  // The expressions in `find` are implicitly `quote`d.

  find q {
    Hi(p)
    q = [p, unquote x]
  }

  return "ok"
}

eval greet("xieyuheng")

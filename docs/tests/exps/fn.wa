print () => {
  return 1
}

print (x) => {
  let y = x
  return y
}

// id

let id = (x) => {
  let y = x
  return y
}

print id(1)
print id(id(1))
print id(id(id(1)))

// zero arity

let one = () => {
  return 1
}

print one()

// relation in function

let greet = (x) => {
  clause Hi("hi")

  // The expressions in `find` are implicitly `quote`d.

  find q {
    Hi(p)
    q = [p, unquote x]
  }

  return "ok"
}

print greet("xieyuheng")
